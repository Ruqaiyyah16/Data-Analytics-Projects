  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "remove_outliers <- function(x) {\n",
    "  qnt <- quantile(x, probs=c(.25,.75))\n",
    "  H <- 1.5 * IQR(x)     \n",
    "  y <- x\n",
    "  y[x < (qnt[1] - H)] <- NA   # to remove the lowerfence\n",
    "  y[x > (qnt[2] + H)] <- NA   # to remove the upperfence\n",
    "  y  \n",
    "}"
   ]
  },
