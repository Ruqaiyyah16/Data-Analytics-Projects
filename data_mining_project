    "ggplot(dataset1, aes(x = status, y = mba_percent)) +\n",
    "  geom_boxplot() +\n",
    "  coord_flip()\n",
    "\n",
    "b0 <- dataset1 %>% ggplot(aes(status,mba_percent))\n",
    "\n",
    "### default box plot\n",
    "b1 <- b0 + geom_boxplot() \n",
    "\n",
    "### box plot color coded\n",
    "b2 <- b0 + geom_boxplot(aes(col = status, fill = status), alpha = 0.25)  \n",
    "\n",
    "b3 <- b2 + geom_jitter(aes(color=status))+ coord_flip()"
